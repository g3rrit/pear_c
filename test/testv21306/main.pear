#include"stdio"

main : () -> int   
{
    printf("test");
    
    bar();
    
    num : i32 = 10;

    return 0;
}

bar : () -> void   
{
    foo : Foo* = new Foo();
    foo->print();
    free(foo);

    foo2 : Foo = crt Foo();
    foo2.print();
}

Foo : struct  
{
    num : i32 = 10;
    num2 : i32 = 50;
    num3 : i64 = 40;
    print : () -> void 
    {
        printf("lol");    
    }
}
